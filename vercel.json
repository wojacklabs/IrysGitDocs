{
  "buildCommand": "npm run docs:build",
  "outputDirectory": ".vitepress/dist",
  "framework": "vitepress",
  "installCommand": "npm install",
  "devCommand": "npm run docs:dev",
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true
} 